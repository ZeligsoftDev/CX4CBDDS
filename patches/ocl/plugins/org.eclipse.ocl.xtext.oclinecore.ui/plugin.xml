				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
    <!-- quickfix marker resolution generator for org.eclipse.ocl.xtext.oclinecore.OCLinEcore -->
   <extension
         point="org.eclipse.ui.ide.markerResolution">
