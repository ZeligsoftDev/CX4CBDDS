<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<plugin>

  <extension point="org.eclipse.core.contenttype.contentTypes">
	<content-type id="org.eclipse.ocl.xtext.OCLinEcore" name="%OCLinEcoreName" 
		base-type="org.eclipse.core.runtime.text"
		file-extensions="oclinecore"
        default-charset="UTF-8">
         <describer class="org.eclipse.ocl.pivot.resource.NotXMLContentHandlerImpl$Describer"/>
      </content-type>
  </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated OCLinEcoreCS -->
      <package
            uri="http://www.eclipse.org/ocl/2015/OCLinEcoreCS"
            class="org.eclipse.ocl.xtext.oclinecorecs.OCLinEcoreCSPackage"
            genModel="model/OCLinEcoreCS.genmodel"/>
   </extension>

  <extension point="org.eclipse.ocl.pivot.as_resource_factory">
     <factory class="org.eclipse.ocl.xtext.oclinecore.utilities.OCLinEcoreASResourceFactory"
     	extension="oclinecore" contentType="org.eclipse.ocl.oclas.oclinecore"/>
  </extension>

   <extension point="org.eclipse.team.core.fileTypes">
      <fileTypes extension="oclinecore" type="text" />
      <fileTypes extension="oclinecorecs" type="text" />
   </extension>

</plugin>
