# Debugging options for the org.eclipse.ocl.examples.debug.vm plug-in

# Trace the source location activity
org.eclipse.ocl.examples.debug.vm/location=false

# Trace the pre-visit activity 
org.eclipse.ocl.examples.debug.vm/pre-visit=false

# Trace the post-visit activity 
org.eclipse.ocl.examples.debug.vm/post-visit=false

# Trace the VisitorStack 
org.eclipse.ocl.examples.debug.vm/visitorStack=false

# Trace VMEvent messaging 
org.eclipse.ocl.examples.debug.vm/vmEvent=false

# Trace VMRequest messaging 
org.eclipse.ocl.examples.debug.vm/vmRequest=false

# Trace VMResponse messaging 
org.eclipse.ocl.examples.debug.vm/vmResponse=false
