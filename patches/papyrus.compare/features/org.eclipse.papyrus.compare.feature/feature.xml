<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.papyrus.compare.feature"
      label="%featureName"
      version="0.7.0.qualifier"
      provider-name="%providerName">

   <description>
      Comparison and merge of Papyrus models, as well as support for managing Papyrus models in Git repositories.
   </description>

