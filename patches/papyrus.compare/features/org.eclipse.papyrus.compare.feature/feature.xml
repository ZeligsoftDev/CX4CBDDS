which accompanies this distribution, and is available at
http://www.eclipse.org/legal/epl-v10.html
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import feature="org.eclipse.emf.compare.diagram.gmf" version="3.0.0" match="compatible" />
      
      <import plugin="org.eclipse.emf.compare" version="3.0.0" match="compatible"/>
      <import plugin="org.eclipse.emf.compare.egit" version="1.1.0" match="compatible"/>
      <import plugin="org.eclipse.emf.compare.ide.ui" version="4.1.0" match="compatible"/>
      <import plugin="org.eclipse.emf.compare.rcp.ui" version="4.0.0" match="compatible"/>
      <import plugin="org.eclipse.emf.compare.uml2" version="2.5.0" match="compatible"/>
   </requires>

   <plugin
         id="org.eclipse.papyrus.compare.uml2"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.papyrus.compare.uml2.edit"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.papyrus.compare.diagram"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.papyrus.compare.diagram.ide.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.papyrus.compare.egit.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
