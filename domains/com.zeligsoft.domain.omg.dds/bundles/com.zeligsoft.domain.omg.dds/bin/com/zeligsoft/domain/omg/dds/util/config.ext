import DDS::Topics;

Void configureTopic(Topic self) :
	let type = new TopicField :
	type.setName("type") ->
	self.setType(type);
	
Void configureQoSProperty(qosProperty self) :
	JAVA com.zeligsoft.domain.omg.dds.util.DDSConfig.configureQoSProperty(org.eclipse.uml2.uml.Property);