<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.emf.ecore.uri_mapping">
      <mapping
            source="pathmap://CCM_LIBRARIES/"
            target="platform:/plugin/com.zeligsoft.domain.omg.ccm/libraries/">
      </mapping>
      <mapping
            source="pathmap://CCM_PROFILES/"
            target="platform:/plugin/com.zeligsoft.domain.omg.ccm/profiles/">
      </mapping>
      <mapping
            source="pathmap://CCM_DOMAIN/"
            target="platform:/plugin/com.zeligsoft.domain.omg.ccm/models/">
      </mapping>
   </extension>
   <extension
         point="com.zeligsoft.base.zdl.domain_models">
      <domainModel
            genModel="pathmap://CCM_DOMAIN/CCM.zdlgen"
            path="pathmap://CCM_DOMAIN/CCM.uml"
            uri="http://www.zeligsoft.com/domain/omg/ccm/2009">
