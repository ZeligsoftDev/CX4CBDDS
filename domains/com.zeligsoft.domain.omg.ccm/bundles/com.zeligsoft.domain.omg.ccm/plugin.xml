   </extension>
   <extension
         point="com.zeligsoft.cx.codegen.workerfunctioncontributor">
      <workerfunctioncontributor
            class="com.zeligsoft.domain.omg.ccm.util.CCMWorkerFunctionContributor"
            concept="CCM::CCM_Implementation::MonolithicImplementation"
            domain="CCM"
            id="com.zeligsoft.domain.omg.ccm.ccmWorkerfunctioncontributor">
      </workerfunctioncontributor>
   </extension>
   <extension
         point="com.zeligsoft.cx.codegen.workerfunctioncontributor">
      <workerfunctioncontributor
            class="com.zeligsoft.domain.zml.util.ZMLWorkerFunctionContributor"
            concept="ZMLMM::ZML_Component::StructuralRealization"
            domain="CCM"
            id="com.zeligsoft.domain.omg.ccm.workerfunctioncontributor">
      </workerfunctioncontributor>
   </extension>
   
   <extension
         point="com.zeligsoft.base.validation.preload">
      <zdl uri="pathmap://CCM_DOMAIN/CCM.uml" />
   </extension>
</plugin>
