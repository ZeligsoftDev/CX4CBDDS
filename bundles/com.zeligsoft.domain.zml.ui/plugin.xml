<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.gmf.runtime.diagram.ui.editpolicyProviders">
      <editpolicyProvider
            class="com.zeligsoft.domain.zml.ui.providers.ZMLComponentEditPolicyProvider">
         <Priority
               name="Highest">
