   </element>

   <element name="includeFile" type="string">
   </element>

   <element name="importOptions" type="string">
   </element>

   <element name="module">
      <complexType>
         <sequence>
            <element ref="generationOptions"/>
            <element ref="portIcon" minOccurs="0" maxOccurs="unbounded"/>
         </sequence>
         <attribute name="name" type="string">
            <annotation>
               <documentation>
                  Name of the module
               </documentation>
            </annotation>
         </attribute>
      </complexType>
   </element>

   <element name="portIcon">
      <annotation>
         <documentation>
            Customize port icons for extended port type.
         </documentation>
      </annotation>
      <complexType>
         <attribute name="name" type="string" use="required">
            <annotation>
               <documentation>
                  Fully qualified name of the port type or interface object in the connector  module that will be customized (e.g., DDS_DCPS::CCM_DDS::Typed::DDS_Write).
               </documentation>
            </annotation>
         </attribute>
         <attribute name="portIcon" type="string" use="required">
            <annotation>
               <documentation>
                  Custom icon for non-confugated port.
               </documentation>
               <appInfo>
                  <meta.attribute kind="resource"/>
               </appInfo>
            </annotation>
         </attribute>
         <attribute name="conjugatedPortIcon" type="string" use="required">
            <annotation>
               <documentation>
                  Custom icon  for confugated port.
               </documentation>
               <appInfo>
                  <meta.attribute kind="resource"/>
               </appInfo>
            </annotation>
         </attribute>
      </complexType>
   </element>

   <element name="portFilteredProperties">
      <annotation>
         <documentation>
            Properties to filter for a given port type.
         </documentation>
      </annotation>
      <complexType>
         <sequence>
            <element ref="filteredProperty" minOccurs="1" maxOccurs="unbounded"/>
         </sequence>
         <attribute name="portTypeName" type="string" use="required">
            <annotation>
               <documentation>
                  The fully qualified name of the port type for which properties will be filtered.
               </documentation>
            </annotation>
         </attribute>
      </complexType>
   </element>

   <element name="filteredProperty">
      <complexType>
         <attribute name="propertyName" type="string" use="required">
            <annotation>
               <documentation>
                  A regular expression determining properties to filter for ports of this type.
               </documentation>
            </annotation>
         </attribute>
      </complexType>
   </element>

