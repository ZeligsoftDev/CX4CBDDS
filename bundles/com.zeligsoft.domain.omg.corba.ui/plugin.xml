            append="true"
            class="com.zeligsoft.domain.omg.corba.ui.providers.CORBACustomPropertySection"
            concept="ZMLMM::ZML_Core::NamedElement"
            property="name">
      </PropertySection>
      <PropertySection
            class="com.zeligsoft.domain.omg.corba.ui.providers.CORBACustomPropertySection"
            concept="CORBADomain::IDL::CORBAOperation"
            property="idlType">
      </PropertySection>
      <PropertySection
            append="false"
            class="com.zeligsoft.domain.omg.corba.ui.providers.CORBACustomPropertySection"
            concept="CORBADomain::IDL::CORBAConstant"
            property="default">
      </PropertySection>
   </extension>
