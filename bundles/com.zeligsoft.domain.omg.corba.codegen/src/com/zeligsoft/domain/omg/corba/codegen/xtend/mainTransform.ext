ScopedName unhandledScopedName(emf::EObject o, CXNamedElement element ) :
	let newScopedName = new ScopedName :
		newScopedName; 
			
ScopedName toScopedName(Void obj) :
	let newScopedName = new ScopedName :
		newScopedName;

ScopedName CORBAPrimitiveKindtoScopedName(String primitive) :
	let newScopedName = new ScopedName :
		switch(primitive)
			{
				case CXPrimitiveKind::CXWChar.literalName():
					newScopedName.name.add("wchar")
				case CXPrimitiveKind::CXVoid.literalName():
					newScopedName.name.add("void")
				case CXPrimitiveKind::CXUnsignedShort.literalName():
					newScopedName.name.add("unsigned short")
				case CXPrimitiveKind::CXUnsignedLongLong.literalName():
					newScopedName.name.add("unsigned long long")
				case CXPrimitiveKind::CXUnsignedLong.literalName():
					newScopedName.name.add("unsigned long")
				case CXPrimitiveKind::CXShort.literalName():
					newScopedName.name.add("short")
				case CXPrimitiveKind::CXOctet.literalName():
					newScopedName.name.add("octet")
				case CXPrimitiveKind::CXObjectRef.literalName():
					newScopedName.name.add("Object")
				case CXPrimitiveKind::CXLongLong.literalName():
					newScopedName.name.add("long long")
				case CXPrimitiveKind::CXLongDouble.literalName():
					newScopedName.name.add("long double")
				case CXPrimitiveKind::CXLong.literalName():
					newScopedName.name.add("long")
				case CXPrimitiveKind::CXFloat.literalName():
					newScopedName.name.add("float")
				case CXPrimitiveKind::CXDouble.literalName():
					newScopedName.name.add("double")
				case CXPrimitiveKind::CXChar.literalName():
					newScopedName.name.add("char")
				case CXPrimitiveKind::CXBoolean.literalName():
					newScopedName.name.add("boolean")
				case CXPrimitiveKind::CXAny.literalName():
					newScopedName.name.add("any")
				case CXPrimitiveKind::CXString.literalName():
					newScopedName.name.add("string")
				case CXPrimitiveKind::CXWString.literalName():
					newScopedName.name.add("wstring")
				case CXPrimitiveKind::CXTypecode.literalName():
					newScopedName.name.add("typecode")
				default:
					newScopedName.name.add("unknown type")
			} -> newScopedName;
			
