<workflow>
	<bean class="org.eclipse.xtend.typesystem.uml2.Setup" />

	<!-- Needed to report issues -->
	<component
		class='org.eclipse.xtend.util.stdlib.ExtIssueReporter' />

	<component id="ngcCDDgenerator"
		file='/workflow/ngcCDDgeneratorNoPostGeneration.mwe'>
		<src-gen value="${src-gen}/" />
	</component>
	
	<component
		class="com.zeligsoft.domain.ngc.ccm.generator.PostGeneration">
		<pathnameSlot value="pathnames" />
	</component>
</workflow>