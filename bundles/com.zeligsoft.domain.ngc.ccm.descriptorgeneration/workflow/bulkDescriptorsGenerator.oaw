<workflow>
	<bean class="org.eclipse.xtend.typesystem.uml2.Setup" />

	<!-- Needed to report issues -->
	<component
		class='org.eclipse.xtend.util.stdlib.ExtIssueReporter' />
	<component id="generator"
		class="com.zeligsoft.domain.ngc.ccm.descriptorgeneration.utils.BulkDescriptorGenerator">
		<srcGen value="${src-gen}/" />
		<pathnamesSlot value="pathnames" />
	</component>
	<component
		class="com.zeligsoft.domain.ngc.ccm.generator.PostGeneration">
		<pathnamesSlot value="pathnames" />
	</component>
</workflow>