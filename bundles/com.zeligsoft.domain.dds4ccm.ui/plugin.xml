   <extension
      point="org.eclipse.papyrus.infra.gmfdiag.canonical.strategies">
	   <semanticChildrenStrategy
          class="com.zeligsoft.domain.dds4ccm.ui.strategies.CompositeDiagramSemanticChildrenStrategy"
          priority="1">
	      <enablement>
	         <and>
	            <with
	                  variable="element">
