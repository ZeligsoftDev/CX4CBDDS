                    </test>
                    <test
                          property="com.zeligsoft.zdl.tester.isZDLConcept"
                          value="CXDomain::IDL::CXStruct">
                    </test>
                 </or>
              </with>
           </visibleWhen>
        </command>
        <command
              commandId="com.zeligsoft.domain.dds4ccm.ui.migratetoaxcioma"
