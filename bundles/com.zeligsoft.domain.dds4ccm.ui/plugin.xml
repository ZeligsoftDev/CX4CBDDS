               <with
                     variable="selection">
                  <test
                        property="com.zeligsoft.zdl.tester.isZDLConcept"
                        value="DDS4CCM::DDS4CCM::DDS4CCMModel">
                  </test>
               </with>
            </visibleWhen>
        </command>
        <command
