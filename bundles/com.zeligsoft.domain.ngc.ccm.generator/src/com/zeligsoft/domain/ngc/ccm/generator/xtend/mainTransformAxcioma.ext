*/
	printIDLSpec(mod.name, spec) ->
	Debug(null, "Leaving processContainer(CXModule mod, Specification spec) \n\tmod.name = " + mod.name + "\n\tspec = " + spec)
;
	
private Void processContainer(uml::Package pkg, Specification spec) :
	let debug = Debug(pkg, "Entering processContainer(uml::Package pkg, Specification spec) \n\tpkg.name = " + pkg.name + "\n\tspec = " + spec) : 
	let definitions = pkg.packagedElement().visitDefinition() :
	Debug(definitions, "Definitions in processContainer(uml::Package pkg, Specification spec) \n\tpkg.name = " + pkg.name + "\n\tspec = " + spec + "\n\tdefinitions = " + definitions) ->
	printIDLSpec(pkg.name + " after visitDefinition", spec) ->
	spec.getEAnnotations().add(pkg.createFileName()) ->
	spec.definitions.addAll(pkg.zdlAsPackage().ownedComment.visitComment()) ->
	printIDLSpec(pkg.name + " after visitComment", spec) ->
	if definitions != Void && !definitions.isEmpty then pkg.processDefnIncludes(spec) ->
	if definitions != Void && !definitions.isEmpty then spec.definitions.addAll(visit(definitions, pkg.eContainer)) ->
	printIDLSpec(pkg.name + " after visit", spec) ->
	Debug(null, "Leaving processContainer(uml::Package pkg, Specification spec) \n\tpkg.name = " + pkg.name + "\n\tspec = " + spec + "\n\tdefinitions = " + definitions)
;

private Void processContainer(CCM::CCM_Core::CCMModel pkg, Specification spec) :
	let debug = Debug(pkg, "Entering processContainer(CCM::CCM_Core::CCMModel pkg, Specification spec) \n\tpkg.name = " + pkg.name + "\n\tspec = " + spec) : 
	let definitions = pkg.packagedElement().visitDefinition() :
	Debug(definitions, "Definitions in processContainer(CCM::CCM_Core::CCMModel pkg, Specification spec) \n\tpkg.name = " + pkg.name + "\n\tspec = " + spec + "\n\tdefinitions = " + definitions) ->
	printIDLSpec(pkg.name + " after visit", spec) ->
	spec.getEAnnotations().add(pkg.createFileName()) ->
	if definitions != Void && !definitions.isEmpty then pkg.processDefnIncludes(spec) ->
	if definitions != Void && !definitions.isEmpty then spec.definitions.addAll(visit(definitions, pkg.eContainer)) ->
	printIDLSpec(pkg.name + " after visit", spec) ->
	Debug(null, "Leaving processContainer(CCM::CCM_Core::CCMModel pkg, Specification spec) \n\tpkg.name = " + pkg.name + "\n\tspec = " + spec + "\n\tdefinitions = " + definitions)
;
	
