create Specification mainTransformAxcioma(DDSMessage self) :
	let idlDDSMessage = self.visitContained() :
		printIDLSpec("[mt DDSMessage 1] self.name = " + self.name, this) ->
		this.getEAnnotations().add(self.createFileName()) ->
		printIDLSpec("[mt DDSMessage 2] self.name = " + self.name, this) ->
		self.processIncludes(this) ->
		printIDLSpec("[mt DDSMessage 3] self.name = " + self.name, this) ->
		this.definitions.add(self.createPragmaNdds()) ->
		printIDLSpec("[mt DDSMessage 4] self.name = " + self.name, this) ->
		this.definitions.addAll(visit(idlDDSMessage, self.eContainer)) ->
		printIDLSpec("[mt DDSMessage 5] self.name = " + self.name, this) ->
		this;


