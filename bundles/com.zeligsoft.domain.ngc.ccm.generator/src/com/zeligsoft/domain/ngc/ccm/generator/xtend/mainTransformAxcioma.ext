private Void processContainer(CXModule mod, Specification spec) :
	let debug = Debug(mod, "Entering processContainer(CXModule mod, Specification spec) \n\tmod.name = " + mod.name + "\n\tspec = " + spec) : 
	mod.eContainer.processContainer(spec) ->
/*
