idl::Definition visit(List[Definition] self, Void container) :
	self;
	
idl::Definition visit(List[Definition] self, emf::EObject container) : 
	self;
	
idl::Definition visit(List[Definition] self, CXModule container) :
	let idlModule = new idl::Module :
		idlModule.setName(container.name) ->
		idlModule.definitions.addAll(self) -> //.copyAll()) ->
		printModule(4, "visit(1) after addAll(self)", idlModule) ->
		idlModule.comments.addAll(container.zdlAsPackage().ownedComment.visitComment()) -> //.copyAll()) ->
		if self.isEmpty then {} else printModuleAndReturn(4, "visit(1) end", idlModule, visit(idlModule, container.eContainer))
		;
		
idl::Definition visit(List[Definition] self, uml::Package container) :
	visit(self, container.eContainer);
		
idl::Definition visit(Definition self, Void container) :
	{self};
	
idl::Definition visit(Definition self, emf::EObject container) : 
	{self};
	
idl::Definition visit(Definition self, CXModule container) :
	let idlModule = new idl::Module :
		idlModule.setName(container.name) ->
		idlModule.definitions.add(self) -> //.copy()) ->
		printModule(4, "visit(2) after addAll(self)", idlModule) ->
		idlModule.comments.addAll(container.zdlAsPackage().ownedComment.visitComment()) -> //.copyAll()) ->
		printModuleAndReturn(4, "visit(2) end", idlModule, visit(idlModule, container.eContainer))
		;

idl::Definition visit(Definition self, uml::Package container) :
	visit(self, container.eContainer);
	
