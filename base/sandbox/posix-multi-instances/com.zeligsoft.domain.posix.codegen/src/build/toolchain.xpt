TOOLCHAINS +=linux_x86_gcc_tools

HOST_OS_linux_x86_gcc_tools:= linux

# important target information
OS_linux_x86_gcc_tools:= linux
PROCESSOR_linux_x86_gcc_tools:= x86
TOOLCHAIN_linux_x86_gcc_tools:= gcc
CF_linux_x86_gcc_tools:= zcf100_sca22
ORB_linux_x86_gcc_tools:= omni410

TARGET_linux_x86_gcc_tools:= $(OS_linux_x86_gcc_tools)_$(PROCESSOR_linux_x86_gcc_tools)_$(TOOLCHAIN_linux_x86_gcc_tools)_$(CF_linux_x86_gcc_tools)_$(ORB_linux_x86_gcc_tools)

# target specific file details
# file extensions and prefixes
OBJEXT_linux_x86_gcc_tools:= .o
EXEEXT_linux_x86_gcc_tools:= 
LIBEXT_linux_x86_gcc_tools:= .a
LIBPREFIX_linux_x86_gcc_tools:= lib
DEPEXT_linux_x86_gcc_tools:= .d

# command options
OBJOPT_linux_x86_gcc_tools:= -o
INCOPT_linux_x86_gcc_tools:= -I
DEFOPT_linux_x86_gcc_tools:= -D
UNDEFOPT_linux_x86_gcc_tools:= -U
IDLOUTOPT_linux_x86_gcc_tools:= -C
IDLINCOPT_linux_x86_gcc_tools:= -I
LIBOUTOPT_linux_x86_gcc_tools:= 
LDOUTOPT_linux_x86_gcc_tools:= -o
LDLIBOPT_linux_x86_gcc_tools:= -l
LDLIBPATHOPT_linux_x86_gcc_tools:= -L

# compiler			
CC_linux_x86_gcc_tools:= gcc

# MAKEDEPEND command
MAKEDEPEND_linux_x86_gcc_tools = $(CC_$(TOOL)) $(CPPFLAGS_linux_x86_gcc_tools) \
  $(foreach def,$(DEFINES),$(DEFOPT_$(TOOL))"$(def)") \
  $(foreach und, $(UNDEFINES),$(UNDEFOPT_$(TOOL))"$(und)") \
  $(foreach inc,$(INCLUDES),$(INCOPT_$(TOOL))"$(subst |, ,$(inc))") \
  $(INCOPT_$(TOOL))$(OBJDIR)/$(TOOL)/$(IDLDIR) \
  -MM -MT $(basename $@)$(OBJEXT_$(TOOL)) -MF $(basename $@)$(DEPEXT_$(TOOL)) $<

# linker
LD_linux_x86_gcc_tools:= gcc

# archiver
AR_linux_x86_gcc_tools:= ar

