<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="com.zeligsoft.base.zdl.rhapsody.test.commands.category"
            name="Sample Category">
      </category>
      <command
            categoryId="com.zeligsoft.base.zdl.rhapsody.test.commands.category"
            id="com.zeligsoft.base.zdl.rhapsody.test.commands.sampleCommand"
            name="Import Rhapsody">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.zeligsoft.base.zdl.rhapsody.test.handlers.ModelImportHandler"
            commandId="com.zeligsoft.base.zdl.rhapsody.test.commands.sampleCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="com.zeligsoft.base.zdl.rhapsody.test.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+6">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="com.zeligsoft.base.zdl.rhapsody.test.menus.sampleMenu"
               label="CX Rhapsody"
               mnemonic="R">
            <command
                  commandId="com.zeligsoft.base.zdl.rhapsody.test.commands.sampleCommand"
                  id="com.zeligsoft.base.zdl.rhapsody.test.menus.sampleCommand"
                  mnemonic="S">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="com.zeligsoft.base.zdl.rhapsody.test.toolbars.sampleToolbar">
            <command
                  commandId="com.zeligsoft.base.zdl.rhapsody.test.commands.sampleCommand"
                  icon="icons/sample.gif"
                  id="com.zeligsoft.base.zdl.rhapsody.test.toolbars.sampleCommand"
                  label="Import Rhapsody"
                  tooltip="Import a Rhapsody model element">
            </command>
         </toolbar>
      </menuContribution>
   </extension>

</plugin>
