«IMPORT uml»

«REM»Linux root makefile«ENDREM»
«DEFINE makefile FOR Model»
«FILE "Linux.mk"»
«EXPAND templates::common::Copyright::copyrightGNUMakefile FOR this»
###################################################################
#
#	Includes
#
###################################################################
include $(SRC_ROOT)Default.mk



###################################################################
#
#	General Variables
#
###################################################################
OBJ_EXT 				= .o
LIB_EXT	 				= .a
LIB_PFX					= lib
DEFINE_SWITCH 			= -D
INCLUDE_SWITCH	 		= -I
LIBINCLUDE_SWITCH 		= -L
PATH_SEP				= /
DELETE_FILE				= rm -rf
LIBRARY_BUILD_EXTENSION = .a


###################################################################
#
#	Compiler Variables
#
###################################################################
CC 				= g++

CFLAGS 				= -Wall -g -c

CDEFINES			=

CINCLUDES 			=

CLIBRARIES 			=

CSRCFILE_SWITCH 		= 

CTRGTFILE_SWITCH 		= -o

###################################################################
#
#	Linker Variables
#
###################################################################

LD                  		= g++

LFLAGS 				= 

LDEFINES 			=

LINCLUDES 			=

LLIBRARIES 			= $(LIBRARY_SWITCH)pthread

export LSRCFILE_SWITCH 		= 

LTRGTFILE_SWITCH 		= -o

export LIBRARY_SWITCH		= -l

export PARTIAL_LD               = ld

export PARTIAL_LINK_OPTIONS     = -r -o

###################################################################
#
#	LIBRARY Variables
#
###################################################################
AR 				= ar

LIBFLAGS 			= -rcs

LIBTRGTFILE_SWITCH  		= 
