«IMPORT uml»

«EXTENSION templates::common»


«DEFINE linxSignalFile FOR Model»
«FILE "fwk/C/transports/LINXSignals/zce.sig"»

/**
 * @file: zce.sig
 * 
 */
#ifndef APP_SIG
#define APP_SIG

#include "ose.h"
#include "zceOperatingEnvironment.h"

/* The data field has the following contents:
 *
 *     Byte           Description
 *     0-4            Number of bytes sent
 *     5-..           Payload
 */
#define SIG_LINX_MSG 0x10
struct 	SigLinxMsgHeader {
    SIGSELECT sig_no;
    char payload[ZCE_LINX_PAYLOAD_SIZE];    	
};		

union SIGNAL {
    SIGSELECT sig_no;
    struct SigLinxMsgHeader msg;
};	


#endif /* ZCE_SIG */
«ENDFILE»
«ENDDEFINE»