            id="com.zeligsoft.domain.sca.agilent.SystemvueModel.presentation.SystemvueModelEditorID"
            name="%_UI_SystemvueModelEditor_label"
            icon="icons/full/obj16/SystemvueModelModelFile.gif"
            extensions="systemvuemodel"
            class="com.zeligsoft.domain.sca.agilent.SystemvueModel.presentation.SystemvueModelEditor"
            contributorClass="com.zeligsoft.domain.sca.agilent.SystemvueModel.presentation.SystemvueModelActionBarContributor">
