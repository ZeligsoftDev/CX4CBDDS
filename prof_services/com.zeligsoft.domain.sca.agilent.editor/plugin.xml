<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="com.zeligsoft.domain.sca.agilent.SystemvueModel.presentation.SystemvueModelModelWizardID"
            name="%_UI_SystemvueModelModelWizard_label"
            class="com.zeligsoft.domain.sca.agilent.SystemvueModel.presentation.SystemvueModelModelWizard"
